<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <authors>$author$</authors>
    <description>$Description$</description>
    <tags>$PackageTags$</tags>

    <!--Project info-->
    <projectUrl>$PackageProjectUrl$</projectUrl>
    <repository type="$RepositoryType$" url="$RepositoryUrl$" branch="master"/>

    <!--License info-->
    <license type="file">$PackageLicenseFile$</license>
    <requireLicenseAcceptance>$PackageRequireLicenseAcceptance$</requireLicenseAcceptance>
    <copyright>$Copyright$</copyright>
    <releaseNotes>$PackageReleaseNotes$</releaseNotes>
    <language>$NeutralLanguage$</language>
  </metadata>

  <files>
    <file src="bin\$Configuration$\$TargetFramework$\ComboPatterns**dll" target="lib\$TargetFramework$" />
    <file src="bin\$Configuration$\$TargetFramework$\ComboPatterns**xml" target="lib\$TargetFramework$" />
    <file src="bin\$Configuration$\$TargetFramework$\$PackageLicenseFile$" target="" />
    <file src="bin\$Configuration$\$TargetFramework$\README.md" target="" />
  </files>
</package>